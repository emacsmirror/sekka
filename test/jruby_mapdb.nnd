(require "sekka/jruby_mapdb")
(use nendo.test)
(load "./test/common.nnd")

(test-start "MapDB (only Java)")
;;===================================================================

;;-------------------------------------------------------------------
(test-section "open DB")

(test* "db open"
       "val1"
       (let1 tree (Jruby::Mapdb::DB.new "test_mapdb" "sekka1")
         (tree.put "key1" "val1")
         (tree.get "key1")))

;;===================================================================


(test-end)
